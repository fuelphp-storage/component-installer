{
	"name": "fuelphp/app-installer",
	"type": "composer-installer",
	"description": "FuelPHP application installer.",
	"keywords": ["FuelPHP", "Application", "Installer"],
	"homepage": "https://github.com/fuelphp/app-installer",
	"license": "MIT",
	"authors": [
		{
			"name": "FuelPHP Development Team",
			"email": "team@fuelphp.com"
		}
	],
	"require": {
		"php": ">=5.3.3"
	},
	"require-dev": {
		"mockery/mockery": "0.7.*"
	},
	"autoload": {
		"psr-0": { "FuelPHP\\AppInstaller": "src/" }
	},
    "extra": {
        "class": "FuelPHP\\AppInstaller\\Installer"
    },
    "minimum-stability": "dev"
}
