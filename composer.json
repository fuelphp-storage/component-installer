{
	"name": "fuelphp/component-installer",
	"type": "composer-installer",
	"description": "FuelPHP Component Installer.",
	"keywords": ["FuelPHP", "Application", "Component", "Installer"],
	"homepage": "https://github.com/fuelphp/component-installer",
	"license": "MIT",
	"authors": [
		{
			"name": "FuelPHP Development Team",
			"email": "team@fuelphp.com"
		}
	],
	"require": {
		"php": ">=5.4"
	},
	"require-dev": {
		"mockery/mockery": "0.7.*"
	},
	"autoload": {
		"psr-4": { "Fuel\\ComponentInstaller\\": "src/" }
	},
    "extra": {
        "class": "Fuel\\ComponentInstaller\\Installer"
    },
    "minimum-stability": "dev"
}
