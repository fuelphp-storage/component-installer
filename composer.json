{
	"name": "fuelphp/package-installer",
	"type": "composer-installer",
	"description": "FuelPHP package installer.",
	"keywords": ["FuelPHP", "Package", "Installer"],
	"homepage": "https://github.com/fuelphp/package-installer",
	"license": "MIT",
	"authors": [
		{
			"name": "FuelPHP Development Team",
			"email": "team@fuelphp.com"
		}
	],
	"require": {
		"php": ">=5.3.3"
	},
	"require-dev": {
		"mockery/mockery": "0.7.*"
	},
	"autoload": {
		"psr-0": { "FuelPHP\\PackageInstaller": "src/" }
	},
    "extra": {
        "class": "FuelPHP\\PackageInstaller\\Installer"
    },
    "minimum-stability": "dev"
}
